{
  "name": "hugo-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "assets:development": "webpack --mode development --progress --hide-modules",
    "assets:production": "webpack --mode production",
    "watch:hugo": "hugo server --buildDrafts --buildExpired --buildFuture",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run watch:hugo\" \"npm run assets:development\"",
    "build": "webpack --mode production",
    "format": "prettier --write src/js/**/*.js *.js",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "purgecss-webpack-plugin": "^1.5.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "terser-webpack-plugin": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "bulma": "^0.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs,jsx,json,scss,css,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
