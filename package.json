{
  "name": "prosightful-counseling",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public/* && rimraf ./static/assets/*",
    "assets:watch": "cross-env NODE_ENV=development webpack --mode development --progress --hide-modules --watch",
    "assets:build": "cross-env NODE_ENV=development webpack -p",
    "hugo:watch": "hugo server --buildDrafts --buildExpired --buildFuture",
    "start": "concurrently \"npm run hugo:watch\" \"npm run assets:watch\"",
    "build": "npm run assets:build && cross-env HUGO_ENV=production hugo",
    "format": "prettier --write src/js/**/*.js *.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "keywords": [
    "hugo"
  ],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    ">1%",
    "not dead"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.6.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "imagemin": "^6.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "terser-webpack-plugin": "^1.3.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "webapp-webpack-plugin": "^2.7.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^3.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "bulma": "^0.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,mjs,jsx,json,scss,css,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
